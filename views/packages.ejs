<%- include('partials/header') %>

<%- include('layout') %>

<h1>Internet Packages</h1>

<table>
    <thead>
        <tr>
            <th>Package ID</th>
            <th>Package Name</th>
            <th>Speed</th>
            <th>Price</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        <% packages.forEach(function(package) { %>
            <tr>
                <td><%= package.id %></td>
                <td><%= package.name %></td>
                <td><%= package.speed %></td>
                <td><%= package.price %></td>
                <td><%= package.details %></td>
            </tr>
        <% }) %>
    </tbody>
</table>





<%- include('partials/footer') %>